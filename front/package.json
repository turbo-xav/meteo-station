        "@angular-devkit/build-angular": "~0.1000.7",
        "@angular/cli": "~10.0.7",
        "@angular/compiler-cli": "~10.0.11",
        "@compodoc/compodoc": "^1.1.11",
        "@types/jest": "^26.0.10",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "ftp-deploy": "^2.3.7",
        "http-server": "^0.12.3",
        "jest": "^26.4.2",
        "jest-html-reporter": "^3.1.3",
