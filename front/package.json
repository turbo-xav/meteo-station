{
    "name": "new-meteo-station-domotique",
    "description": "Station Météo connectée",
    "title": "Station Météo",
    "date": "2021-02-14",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "postinstall": "ngcc",
        "start:dev": "ng serve --open --aot --port=8080",
        "start": "npx http-server ./docs/meteo-station",
        "test": "jest --clearCache && jest --ci",
        "prebuild": "ts-node ./deployment/deploy-date.ts",
        "build": "ng build --prod --baseHref=./ --outputPath=docs/meteo-station",
        "build-test": "npm run build & http-server ./docs/meteo-station -p 8081 -c-1 -o ",
        "deploy": "npm run build && ts-node ./deployment/deploy.ts",
        "deploy-only": "ts-node ./deployment/deploy.ts",
        "deploy-prod": "npm run build && ts-node ./deployment/deploy.ts --prod",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.11",
        "@angular/cdk": "^10.1.3",
        "@angular/common": "~10.0.11",
        "@angular/compiler": "~10.0.11",
        "@angular/core": "~10.0.11",
        "@angular/forms": "~10.0.11",
        "@angular/material": "^10.1.3",
        "@angular/platform-browser": "~10.0.11",
        "@angular/platform-browser-dynamic": "~10.0.11",
        "@angular/router": "~10.0.11",
        "@angular/service-worker": "~10.0.11",
        "@auth0/angular-jwt": "^5.0.1",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "angular-google-charts": "^1.1.4",
        "firebase": "^7.19.0",
